@model JavaTech3.ViewModels.ShippingViewModel
@{
    ViewData["Title"] = "Index";

}

<h1>Index</h1>
<form asp-action="ProductCreate">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <table id="MultiAddUserTable" class="table">
        <tr>
            <th>Name</th>
            <th>Quantity</th>
        </tr>
        @foreach (var item in Model.ShippingProducts)
        {
            <tr>
                <td>@item.Product.Name</td>
                <td>@item.Quantity</td>

            </tr>
        }

    </table>
</form>

<div>
    <a asp-action="Index">Return</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}